<div class="tab-pane active">
  <div class="panel panel-default">
    <ul class="list-group">
      {{#each model.curriculums as |curriculum|}}
          <li class='list-group-item'>
            {{curriculum.title}}
            {{#each curriculum.facts as |fact|}}
              {{fact.title}}
            {{/each}}
          </li>
      {{/each}}
    </ul>
  </div>
</div>
