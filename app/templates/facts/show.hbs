<div class="modal">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" {{action 'closeModal'}} aria-label="Close"><span aria-hidden="true">&times;</span></button>
        <h4 class="modal-title">{{model.title}}</h4>
      </div>
      <div class="modal-body">
        {{model.description}}
        {{ago model.start_date}}
      </div>
    </div>
  </div>
</div>
