{{#if isTeacher}}
  <ul class="list-group">
    {{#each current_user.students as |student|}}
      {{student-item student=student}}
    {{/each}}
    <li class='list-group-item'>{{#link-to 'user.add-student'}}{{fa-icon "plus"}} add student{{/link-to}}</li>
  </ul>

{{else}}
  <h3> Teacher: {{current_user.teacher.fullName}} </h3>
{{/if}}
